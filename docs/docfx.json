      "feedback_github_repo": "OfficeDev/outlook-dev-docs",
      "feedback_product_url": "https://officespdev.uservoice.com/"
    },
    "fileMetadata": {
      "titleSuffix": {
        "**/*.md": "Outlook Developer"
      },
      "author": {
        "add-ins/*.md": "o365devx"
      },
      "ms.author": {
        "add-ins/*.md": "o365devx"
      },
      "ms.technology": {
        "add-ins/*.md": "add-ins"
      },
      "ms.topic": {
